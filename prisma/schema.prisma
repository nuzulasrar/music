  // C - Beam/Girder | M - Steel | T - Permanent Deformations
  beamGirder_steel_perManentDeformations_tick      String    @db.Text
  beamGirder_steel_perManentDeformations_severity      String    @db.Text
  beamGirder_steel_perManentDeformations_percentageAffected1    String    @db.Text
